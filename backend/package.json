{
  "name": "fincobra-server",
  "version": "0.1.0",
  "description": "FinCobra Server",
  "type": "module",
  "scripts": {
    "build": "rm -rf dist && tsc && mkdir -p dist/src/db && cp src/db/schema.sql dist/src/db/schema.sql && cp -r src/db/migrations dist/src/db/migrations",
    "start": "node dist/scripts/start.js",
    "test": "DATABASE_URL=${DATABASE_URL:-postgres://postgres:postgres@localhost:5432/fincobra_test} vitest run",
    "migrate": "node dist/src/db/migrate.js",
    "lint": "eslint .",
    "format": "prettier \"{src,test,scripts}/**/*.{ts,tsx,js,json}\" --write",
    "format:check": "prettier \"{src,test,scripts}/**/*.{ts,tsx,js,json}\" --check"
  },
  "dependencies": {
    "@fastify/cookie": "9.4.0",
    "@fastify/csrf-protection": "6.4.1",
    "@fastify/helmet": "11.1.1",
    "@fastify/rate-limit": "8.1.1",
    "agent-twitter-client": "0.0.18",
    "dotenv": "16.6.1",
    "fastify": "4.29.1",
    "google-auth-library": "10.3.0",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "node-cache": "5.1.2",
    "node-cron": "3.0.2",
    "otplib": "12.0.1",
    "p-limit": "5.0.0",
    "pg": "8.13.0",
    "pino": "8.15.0",
    "qrcode": "1.5.3",
    "rss-parser": "3.13.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "9.33.0",
    "@types/jsonwebtoken": "9.0.10",
    "@types/lodash": "4.17.16",
    "@types/node": "22.18.1",
    "@types/node-cache": "4.2.5",
    "@types/node-cron": "3.0.11",
    "@types/pg": "8.10.2",
    "@types/qrcode": "1.5.5",
    "eslint": "9.33.0",
    "globals": "16.3.0",
    "prettier": "3.6.2",
    "typescript": "5.2.2",
    "typescript-eslint": "8.39.1",
    "vitest": "3.2.4"
  },
  "overrides": {
    "debug": "4.3.4"
  },
  "engines": {
    "node": "22.19.0"
  }
}
